<UserControl x:Class="VideoNS.TimeSwitch.PlusPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:helper="clr-namespace:VideoNS.Helper"
             xmlns:local="clr-namespace:VideoNS.TimeSwitch"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Dictionary/DictIcons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        <local:PlusIconSizeConverter x:Key="PlusIconSizeConverter"/>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grid" Width="45" Height="45">
        <Grid.Background>
            <ImageBrush ImageSource="../Images/plus.png" Stretch="Fill"/>
        </Grid.Background>
    </Grid>
</UserControl>
