<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CCTVReplay">
    <ImageBrush x:Key="PlayBtnBrush"
                ImageSource="../Images/PlayControl/播放.png" />
    <ImageBrush x:Key="PlayBtnBrushMouseOver"
                ImageSource="../Images/PlayControl/播放_mouseover.png" />
    <ImageBrush x:Key="PauseBtnBrushMouseOver"
                ImageSource="../Images/PlayControl/暂停_mouseover.png" />
    <ImageBrush x:Key="PauseBtnBrush"
                ImageSource="../Images/PlayControl/暂停.png" />
    
    <ImageBrush x:Key="StopBtnBrush"
                ImageSource="../Images/PlayControl/停止.png" />
    
    <ImageBrush x:Key="FullScreenBtnBrush"
                ImageSource="../Images/fullscreen.png"/>
    <ImageBrush x:Key="FullScreenExitBtnBrush"
                ImageSource="../Images/fullscreenexit.png"/>

    <ImageBrush x:Key="popupBrush"
                ImageSource="../Images/popup_default.png"/>
    <ImageBrush x:Key="popupoverBrush"
                ImageSource="../Images/popup_mouseover.png"/>
    <ImageBrush x:Key="pullBrush"
                ImageSource="../Images/pull_default.png"/>
    <ImageBrush x:Key="pulloverBrush"
                ImageSource="../Images/pull_mouseover.png"/>
</ResourceDictionary>